{
  "name": "graphiql-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fogarea/graphiql-app"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "tsc --noEmit && eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint ./src --ext ts,tsx --fix",
    "preview": "vite preview",
    "prepare": "husky install",
    "lintstaged": "lint-staged --relative"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write -config .prettierrc"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.2",
    "@types/compose-function": "^0.0.30",
    "@uiw/react-textarea-code-editor": "^2.1.1",
    "compose-function": "^3.0.3",
    "firebase": "^9.21.0",
    "graphql": "^16.6.0",
    "i18next": "^22.4.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.2.2",
    "react-router-dom": "^6.10.0",
    "yup": "^1.1.1",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@types/node": "^18.15.13",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.0"
  }
